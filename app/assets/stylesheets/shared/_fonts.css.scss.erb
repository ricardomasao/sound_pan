@font-face {
  font-family: "CitroenBold";
  src: url(<%= asset_path('citroen_bold.eot')%>);
  src: url("<%= asset_path('citroen_bold.eot?#iefix')%>") format("embedded-opentype"),
       url("<%= asset_path('citroen_bold.woff')%>") format("woff"),
       url("<%= asset_path('citroen_bold.ttf')%>") format("truetype"),
       url("<%= asset_path('citroen_bold.svg#citroen_bold')%>") format("svg");
  font-weight: normal;
  font-style: normal;
}

